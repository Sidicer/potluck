jicofo {
  bridge {
    brewery-jid = "JvbBrewery@auth.%%domain%%"
  }

  sctp {
    enabled = false
  }

  xmpp {
    client {
      enabled = true
      hostname = "localhost"
      port = 5222
      domain = "auth.%%domain%%"
      username = "focus"
      password = "%%keypassword%%"
      conference-muc-jid = "conference.%%domain%%"
    }
  }
}
