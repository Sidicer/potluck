{
	order s3proxy last
	debug
	admin off
}

http://%%domain%%:80 {
        redir https://{host}{uri}
}

https://%%domain%%:443 {
	log
	tls %%email%%
	respond /health-check "OK" 200
	s3proxy /* {
		bucket %%bucket%%
		region global
		index index.html
		endpoint "https://%%server%%:9000"
		force_path_style
	}
}
