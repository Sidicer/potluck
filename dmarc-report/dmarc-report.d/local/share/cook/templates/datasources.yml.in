apiVersion: 1

deleteDatasources:
  - name: Elasticsearch-aggregate
    orgId: 1
  - name: Elasticsearch-forensic
    orgId: 1

datasources:
  - name: Elasticsearch-aggregate
    type: elasticsearch
    access: proxy
    orgId: 1
    url: http://%%ip%%:9200
    database: '[dmarc_aggregate]-YYYY-MM-DD'
    user: admin
    isDefault: 1
    version: 1
    editable: true
    jsonData:
      esVersion: 8.0.0
      interval: Daily
      timeField: '@timestamp'
      tlsSkipVerify: true
      MaxConcurrentShardRequests: 10
      xpack: true
    secureJsonData:
      password: admin
  - name: Elasticsearch-forensic
    type: elasticsearch
    access: proxy
    orgId: 1
    url: http://%%ip%%:9200
    database: '[dmarc_forensic]-YYYY-MM-DD'
    user: admin
    isDefault: 0
    version: 1
    editable: true
    jsonData:
      esVersion: 8.0.0
      interval: Daily
      timeField: '@timestamp'
      tlsSkipVerify: true
      MaxConcurrentShardRequests: 10
      xpack: true
    secureJsonData:
      password: admin
