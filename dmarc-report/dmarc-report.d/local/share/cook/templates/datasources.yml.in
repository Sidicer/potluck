apiVersion: 1

deleteDatasources:
  - name: Elasticsearch
    orgId: 1

datasources:
  - name: Elasticsearch
    type: elasticsearch
    access: proxy
    orgId: 1
    url: http://%%ip%%:9200
    user: %%zincuser%%
    isDefault: 1
    version: 1
    editable: false
    jsonData:
      index: '[dmarc_aggregate-]YYYY-MM-DD'
      interval: Daily
      timeField: '@timestamp'
    secureJsonData:
      password: %%zincpass%%
