apiVersion: 1

deleteDatasources:
  - name: Elasticsearch
    orgId: 1

datasources:
  - name: Elasticsearch
    type: elasticsearch
    access: proxy
    orgId: 1
    url: http://%%ip%%:9200
    database: 'dmarc_aggregate'
    user: %%zincuser%%
    isDefault: 1
    version: 1
    editable: true
    jsonData:
      esVersion: 8.0.0
      interval: Daily
      timeField: '@timestamp'
      tlsSkipVerify: true
      MaxConcurrentShardRequests: 10
      xpack: true
    secureJsonData:
      password: %%zincpass%%
