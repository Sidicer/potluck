jicofo {
  bridge {
    brewery-jid = "JvbBrewery@internal.auth.%%domain%%"
  }
  sctp {
    enabled = false
  }
  xmpp {
    client {
      enabled = true
      hostname = "localhost"
      port = 5222
      domain = "auth.%%domain%%"
      xmpp-domain = "%%domain%%"
      username = "jvb"
      password = "%%keypassword%%"
      conference-muc-jid = "conference.%%domain%%"
      client-proxy = "focus.%%domain%%"
    }
  }
}
