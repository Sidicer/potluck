cache {  use_auto_auth_token = false }

listener "unix" {
  address = "/tmp/vault-agent-unseal.socket"
  tls_disable = true
  socket_mode = "0600"
}

vault {
  address = "https://%%unsealip%%:8200"
  skip_verify = false
  client_cert = "/mnt/unsealcerts/client.crt"
  client_key = "/mnt/unsealcerts/client.key"
  ca_path = "/mnt/unsealcerts/ca_chain.crt"
  tls_server_name = "server.global.vaultunseal"
#  ca_path = "/opt/pot/cluster/certs/ca_chain.crt"
#  client_cert = "/opt/pot/cluster/certs/client.crt"
#  client_key = "/opt/pot/cluster/certs/client.key"
}

log_level="TRACE"
 